{
  "name": "inbox-triage-extension",
  "version": "1.0.0",
  "description": "AI-powered email summarization and reply drafting using on-device models for Gmail and Outlook",
  "type": "module",
  "scripts": {
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:update": "playwright test --update-snapshots",
    "pw:install": "playwright install --with-deps"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "playwright": "^1.40.0"
  },
  "keywords": [
    "chrome-extension",
    "email",
    "ai",
    "summarization",
    "privacy"
  ],
  "author": "Mitchell Harrison",
  "license": "MIT"
}